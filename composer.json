{
  "name": "exts/canister",
  "description": "PHP 7 dependency autowiring psr-11 container",
  "require": {
    "php": "^7.0",
    "psr/container": "^1.0",
    "psr/simple-cache": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1.0"
  },
  "autoload": {
    "psr-4": {
      "Canister\\": "src",
      "Test\\": "tests"
    },
    "files": [
      "functions.php"
    ]
  }
}